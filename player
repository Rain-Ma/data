public class Player
{
    private int points;
    private double risk;
  public Player()
  {
    int points =0;
  }
  
  public Player(int x, double y)
  {
    points =x;
    risk = y;
    }
  
  public int getPoints()
  {
    return points;
  }
  
  public double getRisk()
  {
    return risk;
  }
  
  public void addPoints(int x)
  {
    points = points+x;
  }
  
  public void setRisk(double x)
  {
    risk = x;
    }
  



}
